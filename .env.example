# AWS Configuration
AWS_REGION=us-east-2
AWS_ACCESS_KEY_ID=your_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
AWS_ACCOUNT_ID=123456789012

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
BEDROCK_AGENT_ID=your_agent_id
BEDROCK_AGENT_ALIAS_ID=your_agent_alias_id
BEDROCK_KB_ID=your_knowledge_base_id

# DynamoDB Tables
CONTRACTS_TABLE=ContractGuard-Contracts
SESSIONS_TABLE=ContractGuard-NegotiationSessions
CLAUSES_TABLE=ContractGuard-ClauseLibrary
USERS_TABLE=ContractGuard-Users
APPROVALS_TABLE=ContractGuard-PendingApprovals

# S3 Buckets
CONTRACTS_BUCKET=contractguard-contracts-bucket
DOCUMENTS_BUCKET=contractguard-documents-bucket
KB_BUCKET=contractguard-knowledge-base

# Email Configuration (Amazon SES)
SES_FROM_EMAIL=noreply@contractguard.ai
SES_REGION=us-east-1
SES_CONFIGURATION_SET=contractguard-emails

# API Configuration
API_GATEWAY_URL=https://your-api-id.execute-api.us-east-1.amazonaws.com/prod
API_KEY=your_api_key_here

# Authentication
COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
COGNITO_CLIENT_ID=your_client_id
COGNITO_DOMAIN=contractguard-auth

# Application Settings
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO
MAX_FILE_SIZE_MB=25

# Feature Flags
ENABLE_EMAIL_SENDING=false
ENABLE_LEARNING=true
ENABLE_REDLINE_CREATION=true